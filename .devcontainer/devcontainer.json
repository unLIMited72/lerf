{
    "name": "NeRF_Studio-LERF Dev",
    "image": "nerf-studio_image:0.3.4",
    "workspaceFolder": "/workspace",
    "shutdownAction": "stopContainer",
    "runArgs": [
        "--gpus", "all",
        "--shm-size=80gb",
        "-p", "7007:7007",
        "-v", "${localWorkspaceFolder}:/workspace",
        "-v", "/home/${localEnv:USER}/.cache:/home/user/.cache"
    ]
}
